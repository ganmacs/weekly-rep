<div class="sub_element">
  <div class="sub_header">
    <span class="glyphicon glyphicon-user" aria-hidden="true"></span> Members
  </div>
  <div class="sub_body">
    <% members.each do |member| %>
      <div class="sub_body_elem">
        <span class="min_icon"><%= gravatar_for member, 16 %></span>
        <%= link_to_users_weekly_reports(member) %>
      </div>
    <% end %>
  </div>
</div>
